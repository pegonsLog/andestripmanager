{
  "name": "andestripmanager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headless": "cypress run --headless",
    "e2e:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "e2e:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "e2e:ci": "start-server-and-test start http://localhost:4200 e2e:headless",
    "e2e:full": "./scripts/run-e2e-tests.sh",
    "e2e:full:mobile": "./scripts/run-e2e-tests.sh local mobile",
    "e2e:full:tablet": "./scripts/run-e2e-tests.sh local tablet",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:prod": "./scripts/deploy-production.sh",
    "backup": "./scripts/backup.sh",
    "monitor": "./scripts/monitor.sh",
    "precommit": "npm run lint && npm run test:ci",
    "prebuild": "npm run lint",
    "postbuild": "npm run analyze",
    "analyze": "npx webpack-bundle-analyzer dist/andestripmanager/stats.json",
    "serve:prod": "npx http-server dist/andestripmanager -p 8080 -c-1",
    "icons:generate": "node generate-icons.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.14",
    "@angular/cdk": "18.2.14",
    "@angular/common": "18.2.14",
    "@angular/compiler": "18.2.14",
    "@angular/core": "18.2.14",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "18.2.14",
    "@angular/localize": "18.2.14",
    "@angular/material": "18.2.14",
    "@angular/platform-browser": "18.2.14",
    "@angular/platform-browser-dynamic": "18.2.14",
    "@angular/router": "18.2.14",
    "@angular/service-worker": "18.2.14",
    "firebase": "^10.14.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.20",
    "@angular-devkit/core": "^18.2.0",
    "@angular-eslint/builder": "^18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.0",
    "@angular-eslint/schematics": "^18.0.0",
    "@angular-eslint/template-parser": "^18.0.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "18.2.14",
    "@cypress/schematic": "^4.1.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^15.0.0",
    "eslint": "^9.0.0",
    "firebase-tools": "^13.0.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.4",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.4.0",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
